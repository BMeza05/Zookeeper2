package com.brian.zookeeper2;

public class BatTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Bat yungboa = new Bat();
		
		yungboa.fly(50);
		yungboa.attackTown(100);
		yungboa.eatHuamns(25);
		yungboa.attackTown(100);
		yungboa.eatHuamns(25);
		yungboa.fly(50);
		yungboa.attackTown(100);
	}

}
